- links.each do |link|
  .row
    .col-sm-12
  .row
    .col-sm-1
      .thumbnail
        %img.img-responsive.user-photo{:src => "https://ssl.gstatic.com/accounts/ui/avatar_2x.png"}
    .col-sm-5
      .panel.panel-default
        .panel-heading
          %strong= link.user.username
          %span.text-muted= time_ago_in_words(link.created_at)
        .panel-body
          = link_to URI.extract(link.name), URI.extract(link.name)
